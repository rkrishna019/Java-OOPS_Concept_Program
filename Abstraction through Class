abstract class Bank{
    abstract float rate_of_interest(float rate);
}
class SBI extends Bank{
    float rate_of_interest(float rate){
        System.out.println("Rate of Interest is"+ rate);
        return rate;
    }
}
class BOI extends Bank{
    float rate_of_interest(float rate){
        System.out.println("Rate of Interest is"+ rate);
        return rate;
    }
}
class Indian extends Bank{
    float rate_of_interest(float rate){
        System.out.println("Rate of Interest is"+ rate);
        return rate;
    }
}
class Test{
    public static void main(String args[]){
        Bank b;
        b = new SBI();
        b.rate_of_interest(10.2f);
        
        b = new BOI();
        b.rate_of_interest(12.2f);
     
        b = new Indian();
        b.rate_of_interest(18.9f);
        
    }
}
